@model wellcare.Models.ElderProfileViewModel

<h2>Elder Profile</h2>

<table border="1" cellpadding="8">
    <tr>
        <td><b>Name</b></td>
        <td>@Model.ElderName</td>
    </tr>
    <tr>
        <td><b>Email</b></td>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <td><b>Age</b></td>
        <td>@Model.Age</td>
    </tr>
    <tr>
        <td><b>Gender</b></td>
        <td>@Model.Gender</td>
    </tr>
    <tr>
        <td><b>Linked At</b></td>
        <td>@Model.LinkedAt</td>
    </tr>
</table>

<br />

<a asp-controller="CaretakerHome" asp-action="Index">
    ⬅ Back to Caretaker Home
</a>
